{
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  // Remove .fvm files from search
  "search.exclude": {
    "**/.fvm": true
  },
  // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "bloc.newBlocTemplate.type": "equatable",
  "bloc.newCubitTemplate.type": "equatable",
  "bloc.newCubitTemplate.createDirectory": false,
  "bloc.newBlocTemplate.createDirectory": false
}
